{
  "name": "erealite",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "google-map-react": "^1.0.5",
    "node-sass": "^4.9.2",
    "node-sass-chokidar": "^1.3.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-paypal-express-checkout": "^1.0.4",
    "react-rangeslider": "^2.2.0",
